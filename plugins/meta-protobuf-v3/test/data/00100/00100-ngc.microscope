scanner3d {
  scanner2d {
    frequency_hz: 7920
    nscans: 256
    line_duty_cycle: 0.99
    daq {
      kind: NIDAQ
      nidaq {
        ao_samples_per_waveform: 16384
        trigger: "APFI0"
        ctr: "ctr0"
        armstart: "PFI15"
        name: "Dev2"
        level_volts: 0.5
        flyback_scans: 4
        frame_trigger_out: "PFI1"
      }
    }
    digitizer {
      kind: Alazar
      alazar {
        system_id: 1
        sample_rate: SAMPLE_RATE_125MSPS
        channels {
          board_id: 0
          chan_id: CHANNEL_A
          range: INPUT_RANGE_PM_1_V
          enabled: true
        }
        aux_out_board_id: 1
      }
    }
    pockels {
      kind: NIDAQ
      nidaq {
        v_lim_max: 2
        v_lim_min: 0
        v_open: 1.2
        v_closed: 0
        ao_channel: "ao2"
        ai_channel: "ai16"
      }
    }
    shutter {
      kind: NIDAQ
      nidaq {
        open: 1
        closed: 0
        do_channel: "/Dev2/port0/line8"
      }
    }
    linear_scan_mirror {
      kind: NIDAQ
      nidaq {
        v_lim_max: 10
        v_lim_min: -10
        vpp: 20
        ao_channel: "ao0"
      }
    }
  }
  zpiezo {
    kind: NIDAQ
    nidaq {
      um2v: 0.025
      v_lim_max: 12
      v_lim_min: -3
      v_offset: 0
      channel: "ao1"
    }
    um_max: 399
    um_min: 0
    um_step: 1
  }
  zref_um: 0
}
stage {
  kind: C843
  c843 {
    id: 1
    axis {
      id: 1
      stage: "M-511.DD_NEW_X"
    }
    axis {
      id: 2
      stage: "M-511.DD_NEW_Y"
    }
    axis {
      id: 3
      stage: "M-501.1DG"
    }
  }
  simulated {
    axis {
      min_mm: 0
      max_mm: 100
    }
    axis {
      min_mm: 0
      max_mm: 100
    }
    axis {
      min_mm: 0
      max_mm: 12.5
    }
  }
  tilemode: PixelAligned
  default_velocity_mm_per_sec {
    x: 4
    y: 4
    z: 1
  }
  last_target_mm {
    x: 87.864006
    y: 31.2
    z: 9.75
  }
}
vibratome {
  kind: Serial
  serial {
    port: "COM4"
    amplitude: 255
