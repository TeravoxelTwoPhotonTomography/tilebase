scanner3d {
  scanner2d {
    frequency_hz: 7920
    nscans: 128
    line_duty_cycle: 0.99
    daq {
      kind: NIDAQ
      nidaq {
        ao_samples_per_waveform: 16384
        trigger: "APFI0"
        ctr: "ctr0"
        armstart: "PFI15"
        name: "Dev2"
        level_volts: 0.5
        flyback_scans: 4
        frame_trigger_out: "PFI1"
      }
    }
    digitizer {
      kind: Alazar
      alazar {
        system_id: 1
        sample_rate: SAMPLE_RATE_125MSPS
        channels {
          board_id: 0
          chan_id: CHANNEL_A
          range: INPUT_RANGE_PM_1_V
          enabled: true
        }
        channels {
          board_id: 0
          chan_id: CHANNEL_B
          range: INPUT_RANGE_PM_1_V
          enabled: true
        }
        aux_out_board_id: 1
      }
    }
    pockels {
      kind: NIDAQ
      laser: Chameleon
      name: "Chameleon"
      calibration {
        calibrated: true
        v_zero: 0
        v_max: 0.75
      }
      nidaq {
        v_lim_max: 2
        v_lim_min: 0
        v_open: 0.75
        v_closed: 0
        ao_channel: "ao2"
        ai_channel: "ai16"
      }
      simulated {
      }
    }
    pockels {
      kind: NIDAQ
      laser: Fianium
      name: "Fianium (1064nm)"
      calibration {
        calibrated: true
        v_zero: 0
        v_max: 1.5
      }
      nidaq {
        v_lim_max: 2
        v_lim_min: 0
        v_open: 0
        v_closed: 0
        ao_channel: "ao3"
        ai_channel: ""
      }
      simulated {
      }
    }
    shutter {
      kind: NIDAQ
      nidaq {
        open: 1
        closed: 0
        do_channel: "/Dev2/port0/line8"
      }
    }
    linear_scan_mirror {
      kind: NIDAQ
      nidaq {
        v_lim_max: 10
        v_lim_min: -10
        vpp: 20
        ao_channel: "ao0"
      }
    }
  }
  zpiezo {
    kind: NIDAQ
    nidaq {
      um2v: 0.025
      v_lim_max: 12
      v_lim_min: -3
      v_offset: 0
      channel: "ao1"
    }
    um_max: 398
    um_min: 0
    um_step: 2
  }
  zref_um: 0
}
